<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/Differencebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="315dp"
        android:layout_marginTop="119dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="casual"
        android:text="CO2"
        android:textColor="#0E0E0E"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tvCartItemCurrentCo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="313dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="20dp"
        android:layout_toStartOf="@+id/addItemButton"
        android:fontFamily="casual"
        android:text="CO2"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tvCartItemAltCo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="315dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="20dp"
        android:layout_toStartOf="@+id/addItemButton"
        android:fontFamily="casual"
        android:text="CO2"
        android:textColor="#020202"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tvCartItemAlt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="20dp"
        android:layout_toStartOf="@+id/addItemButton"
        android:fontFamily="casual"
        android:text="Food Item"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tvFoodText5"
        android:layout_width="199dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="119dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="20dp"
        android:layout_toStartOf="@+id/addItemButton"
        android:fontFamily="casual"
        android:text="Reduction of CO2"
        android:textColor="#050000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tvCartItemCurrent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="20dp"
        android:layout_toStartOf="@+id/addItemButton"
        android:fontFamily="casual"
        android:text="Food Item"
        android:textColor="#070707"
        android:textSize="14sp" />

</RelativeLayout>